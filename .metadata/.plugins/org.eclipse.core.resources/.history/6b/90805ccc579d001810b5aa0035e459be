package com.prpl.model;
@Entity
@Table(name="employee001")
public class Employee {
	
	 @Id
	    @GeneratedValue(strategy = GenerationType.AUTO)
	    private int id;
	 
	    @Column
	    private String name;
	 
	    @Column
	    private String email;
	 
	    @Column
	    private String address;
	 
	    @Column
	    private String telephone;
	 

}
